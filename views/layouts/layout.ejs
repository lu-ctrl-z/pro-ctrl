<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="vi" lang="vi" dir="ltr">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
<meta http-equiv="Content-Style-Type" content="text/css" />
<meta http-equiv="Content-Script-Type" content="text/javascript" />
<title><%=sails.config.common.site_title%></title>
<link href="/bootstrap/css/bootstrap.min.css?v2" rel="stylesheet" type="text/css">
<link href="/css/style.css" rel="stylesheet" type="text/css">
<link href="/css/style.cmd.css" rel="stylesheet" type="text/css">
<%include ../element/_init%>
<script src="/js/ICONFIG.js"></script>
<script src="/js/jquery.min.js"></script>
<script src="/js/angular.js"></script>
<script src="/bootstrap/js/bootstrap.min.js"></script>
<script src="/js/__d.js"></script>
<script src="/js/__c.js"></script>
<script src="/js/__ui.js"></script>
<script src="/js/document.ready.js"></script>
</head>
<body id="appManager" class="appManager" data-ng-app="app">
<%include MenubarContainer%>
<%include ToolbarContainer%>
<%include LeftSideMainFormControl%>
<%include RightSidebarContainer%>
<div data-ng-controller="tabsCtrl" id="tabsCtrl">
    <div id="tabsList" class="tabsList default-left" style="right: 240px;">
        <ul class="iTabs clearfix" data-ng-if="tabs.length > 0">
            <li class="iTabs-item" data-ng-repeat="tab in tabs" data-ng-class="{isActive: tab.isActive}"
                ng-bind-html="addElement(tab) | NE" data-ng-click="setActive(tab)"
            >
            </li>
        </ul>
    </div>
    <div id="MainContainer" class="MainContainer tran default-left" style="right: 240px;">
        <%- body %>
    </div>
</div>
</body>
</html>